<Window x:Class="PDStat.ManageEditSongs"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Title="Manage Edit Songs" Icon="{StaticResource icon}" Height="336.164" Width="300" ResizeMode="NoResize">
	<Grid>
		<Label Content="Game" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="37" Width="70" FontSize="20"/>
		<ComboBox x:Name="gamesBox" HorizontalAlignment="Center" Margin="0,52,0,0" VerticalAlignment="Top" Width="274" Height="23" Loaded="gamesBox_Loaded" SelectionChanged="gamesBox_SelectionChanged"/>
		<Label Content="Title of Edit song" HorizontalAlignment="Left" Margin="10,80,0,0" VerticalAlignment="Top" Height="37" FontSize="20" Width="163"/>
		<TextBox x:Name="editTitleBox" HorizontalAlignment="Center" Margin="0,122,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="274" FontSize="16"/>
		<Button x:Name="addEditButton" Content="Add song" HorizontalAlignment="Center" Margin="0,154,0,0" VerticalAlignment="Top" Width="74" Click="addEditButton_Click"/>
		<ComboBox x:Name="existingEditSongs" HorizontalAlignment="Center" Margin="0,240,0,0" VerticalAlignment="Top" Width="274" IsEnabled="False"/>
		<Button x:Name="removeEditSongButton" Content="Remove" HorizontalAlignment="Center" Margin="0,270,0,0" VerticalAlignment="Top" Width="74" IsEnabled="False" Click="removeEditSongButton_Click"/>

	</Grid>
</Window>
