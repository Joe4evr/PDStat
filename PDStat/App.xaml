<Application x:Class="PDStat.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 StartupUri="MainWindow.xaml">
	<Application.Resources>
		<ResourceDictionary>
			<BitmapImage x:Key="icon" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="Resources/icon.ico"/>
			<BitmapImage x:Key="bg" CreateOptions="IgnoreImageCache" CacheOption="OnLoad" UriSource="Resources/bg.png"/>
		</ResourceDictionary>
	</Application.Resources>
</Application>
